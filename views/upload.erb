<%= flash[:notice] %>

<h1>Select a zipfile (or not) to upload</h1>

<form action="upload" method="POST" enctype="multipart/form-data">
  <input type="file" name="file">
  <input type="submit" value="Upload!">
</form>

<%= form_tag('/uploads', method: :post) do
  input_tag :movie_title
  submit_tag 'Upload!'
end %>
